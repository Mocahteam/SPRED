importer jeu, entréeSortie, listeConstantesKP41;

programme mission6
glossaire
    unité <Unité>;
    pos <Position>;
    i <Entier>;
début
    ouvrirJeu;
    pos.x <- 256.0;
    pos.y <- 811.0;
    i <- 1;
    tantque i <= nombreUnités(MOI) faire
        unité <- ièmeUnité(MOI, i);
        si unité.sorte = ASSEMBLER alors
            réaliserActionSurPosition (unité, MOVE, pos);
        fin si;
        i <- i + 1;
    fin tantque;
    fermerJeu;
fin
